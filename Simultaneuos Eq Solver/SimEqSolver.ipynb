    "import numpy as np\n",
    "\n",
    "# Define the coefficients of the equations\n",
    "A = np.array([[2, 3], [4, 1]])\n",
    "B = np.array([8, 10])\n",
    "\n",
    "# Solve the equations\n",
    "X = np.linalg.solve(A, B)\n",
    "\n",
    "# Print the solution\n",
    "print(\"Solution:\")\n",
    "print(\"x =\", X[0])\n",
    "print(\"y =\", X[1])\n"
   ]
